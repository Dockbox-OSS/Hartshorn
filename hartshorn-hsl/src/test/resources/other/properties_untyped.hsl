class Vector {}

var v1 = Vector();
v1.x = 10;
v1.y = 20;

var v2 = Vector();
v2.x = 20;
v2.y = 10;

fun sumVector(){
   var result = Vector();
   result.x = v1.x + v2.x;
   result.y = v1.y + v2.y;
   return result;
}

// Re-declare v2 for following statements
var v2 = sumVector();

print("V2 x : " + v2.x);
print("V3 x : " + v2.y);