class Car {
   private name;

   constructor(name) {
      this.name = name;
   }

   fun methodShowInfo() {
      print("Car name from Method : " + this.name);
   }
}

fun Car:extensionShowInfo(){
   print("Car name from Extension : " + this.name);
}

var carArray = [null] * 3;
for (var i=0; i<carArray.length; i++) {
    carArray[i] = Car("Car " + i);
}

for (var car in carArray) {
    car.methodShowInfo();
    car.extensionShowInfo();
}
