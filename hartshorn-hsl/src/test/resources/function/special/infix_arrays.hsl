var length = 2;

fun arrayContains(arr, obj) {
   var index = 0;
   while (index < length) {
      if (arr[index] == obj) {
          return true;
      }
      index = index + 1;
   }
   return false;
}

var arr = ["One", "Two"];
infix fun contains(arr, obj) {
    return arrayContains(arr, obj);
}

print(arr contains "One");
